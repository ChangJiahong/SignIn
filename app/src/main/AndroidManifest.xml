<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.cjh.signin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <!-- SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 向SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 向SDCard读取数据权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.IndexActivity"
            android:theme="@style/NoTitleTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:label="登陆"
            android:theme="@style/NoTitleTheme1" />
        <activity
            android:name=".Activity.MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".Activity.SignInActivity" />
        <activity android:name=".Activity.StuTableActivity" />
        <activity
            android:name=".Activity.StuListActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".Activity.TableActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="file" />
                <data android:scheme="content" />
                <data android:mimeType="application/vnd.ms-excel" />
                <data android:mimeType="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Table2Activity" />
        <activity android:name=".Activity.AddActivity" />
        <activity android:name=".Activity.SignOldActivity" />
        <activity
            android:name=".Activity.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".Activity.AboutMe" />
        <activity
            android:name=".Activity.SignInByFace"
            android:screenOrientation="landscape"
            android:theme="@style/NoTitleTheme" />
        <activity android:name=".Activity.MyClass" />
        <activity android:name=".Activity.StuList" />
        <activity android:name=".Activity.StuInfoActivity" />
        <activity
            android:name=".Activity.HelpActivity"
            android:label="@string/title_activity_help" />
        <activity
            android:name=".Activity.UserInfo"
            android:label="@string/title_activity_user" />
        <activity
            android:name=".Activity.UpName"
            android:label="@string/title_activity_name" />
        <activity
            android:name=".Activity.UpPwd"
            android:label="@string/title_activity_pwd" />
        <activity
            android:name=".Activity.YunActivity"
            android:label="@string/title_activity_yun" />
        <activity
            android:name=".Activity.RegisterActivity"
            android:theme="@style/NoTitleTheme1" />
        <activity android:name=".Activity.HistoryActivity" />
        <activity android:name=".Activity.ClassActivity" />
        <activity android:name=".Activity.SelectActivity" />
        <activity android:name=".Activity.GPAActivity" />
        <activity android:name=".Activity.GListActivity" />
        <activity android:name=".Activity.TestListActivity" />
        <activity android:name=".Activity.TestActivity" />
        <activity android:name=".Activity.StatisticsActivity" />
        <activity android:name=".Activity.StatisticsClassActivity"></activity>
    </application>

</manifest>